  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "protocol": "inspector",
      "program": "${workspaceRoot}/dist/ts2fable.js",
      "args":[
        "${workspaceRoot}/node_modules/reactxp/dist/web/ReactXP.d.ts",
        "${workspaceRoot}/test-compile/ReactXP.fs",
        "-d dist"
      ],
      "sourceMaps": true,
      "preLaunchTask": "build",
      "outFiles": []
    },
    {
      "type": "node",
      "request": "attach",
      "name": "Attach to Port",
      "address": "localhost",
      "port": 5858,
      "outFiles": []
    }
  ]
}