{
  "name": "ts2fable",
  "version": "0.1.23",
  "description": "TypeScript definition files parser for fable-compiler",
  "main": "./orig/ts2fable.js",
  "bin": {
    "ts2fable": "./orig/ts2fable.js"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "ver": "tsc -v",
    "run-new": "ts-node ./dist/ts2fable ./node_modules/@types/electron/index.d.ts",
    "run-orig": "ts-node ./orig/ts2fable.js ./node_modules/@types/electron/index.d.ts",
    "trash": "trash './output/**/*.fs'",
    "run-both": "cross-env yarn build && yarn trash && yarn run-new > ./output/electron-new.fs && yarn run-orig > ./output/electron-orig.fs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fable-compiler/Fable/src/ts2fable"
  },
  "keywords": [
    "fable",
    "fable-compiler",
    "fsharp",
    "F#",
    "TypeScript"
  ],
  "author": "Alfonso Garcia-Caro (https://twitter.com/alfonsogcnunez)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/fable-compiler/Fable/issues"
  },
  "homepage": "https://github.com/fable-compiler/Fable#readme",
  "dependencies": {
    "@types/electron": "^1.4.35",
    "@types/node": "^7.0.12",
    "@types/react": "^15.0.20",
    "byots": "^2.3.0-dev.20170328.5.14",
    "cross-env": "^4.0.0",
    "trash-cli": "^1.4.0",
    "ts-node": "^3.0.2",
    "tslint": "^4.9.0",
    "tslint-config-standard": "^4.0.0",
    "typescript": "2.1"
  }
}
